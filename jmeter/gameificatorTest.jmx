                <stringProp name="script">try {
	
	String key = vars.get(&quot;applicationKey&quot;);
	String secret = vars.get(&quot;applicationSecret&quot;);
	String url = sampler.getUrl().toString();
	
	String data = key+url;
	
	
	String hmacSha1 = org.apache.commons.codec.digest.HmacUtils.hmacSha1Hex(secret, data);
	byte[] b64Signature = org.apache.commons.codec.binary.Base64.encodeBase64(hmacSha1.getBytes());
	String signature = new String(b64Signature);
	vars.put(&quot;signature&quot;,signature);
}
